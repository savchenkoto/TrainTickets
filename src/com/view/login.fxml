<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="240.0" prefWidth="264.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.LoginController">
  <children>
    <AnchorPane prefHeight="224.0" prefWidth="225.0">
      <children>
        <Label layoutX="27.0" layoutY="32.0" text="Добро Пожаловать" textAlignment="CENTER" textFill="#8d8585">
          <font>
            <Font name="FreeSans" size="24.0" />
          </font>
        </Label>
        <TextField fx:id="username" alignment="CENTER" layoutX="40.0" layoutY="70.0" promptText="Ваше имя">
          <font>
            <Font size="14.0" />
          </font>
        </TextField>
        <PasswordField fx:id="passId" alignment="CENTER" layoutX="40.0" layoutY="112.0" prefHeight="26.0" prefWidth="185.0" promptText="Пасспортные данные">
          <font>
            <Font size="14.0" />
          </font>
        </PasswordField>
        <Button fx:id="loginButton" layoutX="40.0" layoutY="152.0" mnemonicParsing="false" onAction="#login" prefHeight="28.0" prefWidth="185.0" text="Войти">
          <font>
            <Font size="14.0" />
          </font>
        </Button>
        <Button layoutX="59.0" layoutY="188.0" mnemonicParsing="false" onAction="#register" prefHeight="26.0" prefWidth="147.0" text="Регистрация" />
      </children>
    </AnchorPane>
  </children>
</StackPane>
