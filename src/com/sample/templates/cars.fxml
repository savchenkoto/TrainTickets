<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<SplitPane dividerPositions="0.8038147138964578" orientation="VERTICAL" prefHeight="424.0" prefWidth="778.0"
           xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141"
           fx:controller="com.controller.CarController">
  <items>
    <StackPane prefHeight="150.0" prefWidth="200.0">
      <children>
        <TableView fx:id="carTable" prefHeight="200.0" prefWidth="200.0">
          <columns>
            <TableColumn fx:id="trainColumn" prefWidth="277.0" text="Train"/>
            <TableColumn fx:id="numberColumn" prefWidth="294.0" text="Number"/>
            <TableColumn fx:id="typeColumn" prefWidth="204.0" text="Class"/>
          </columns>
        </TableView>
      </children>
    </StackPane>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="64.0" prefWidth="770.0">
      <children>
        <ComboBox fx:id="trainComboBox" layoutX="14.0" layoutY="21.0" prefHeight="26.0" prefWidth="118.0"
                  promptText="Train" AnchorPane.leftAnchor="14.0"/>
        <ComboBox fx:id="typeComboBox" layoutX="144.0" layoutY="23.0" prefHeight="26.0" prefWidth="118.0"
                  promptText="Class"/>
        <TextField fx:id="numberField" layoutX="270.0" layoutY="23.0" prefHeight="26.0" prefWidth="118.0" promptText="Number"/>
        <Button layoutX="403.0" layoutY="23.0" mnemonicParsing="false" onAction="#addButton" text="Add"/>
        <Button onAction="#updateButton" layoutX="457.0" layoutY="23.0" mnemonicParsing="false" text="Update"/>
        <Button onAction="#deleteButton" layoutX="532.0" layoutY="23.0" mnemonicParsing="false" text="Delete"/>
      </children>
    </AnchorPane>
  </items>
</SplitPane>
